  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "@vanilla-extract/css": "1.7.0",
    "@vanilla-extract/esbuild-plugin": "2.0.4",
    "esbuild": "0.14.38",
    "esbuild-jest": "0.5.0",
    "eslint": "8.14.0",
    "jest": "28.0.3",
    "prettier": "2.6.2",
    "shx": "0.3.4",
    "tsup": "5.12.6",
    "typescript": "4.6.4"
