{
  "name": "@bodo/modal",
  "version": "0.0.0",
  "description": "Bodo Design System: Modal Component",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
